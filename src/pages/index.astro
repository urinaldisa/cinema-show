---
export const prerender = false;
import { Toaster } from "sonner";
import Layout from "../layouts/Layout.astro";
import AppRoot from "../sections/AppRoot";
const token = Astro.cookies.get("auth_token")?.value ?? null;
if (!token) {
  return Astro.redirect("/login");
}
---

<Layout>
	<AppRoot client:load />
	<Toaster richColors position="top-right" closeButton client:load />
</Layout>
